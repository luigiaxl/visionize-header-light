{
  "name": "@luigiaxl/visionize-header-light",
  "version": "0.1.0",
  "description": "Visionize header component",
  "main": "dist/VisionizeHeaderLight.umd.js",
  "module": "dist/VisionizeHeaderLight.esm.js",
  "unpkg": "dist/VisionizeHeaderLight.min.js",
  "browser": {
    "./sfc": "src/VisionizeHeaderLight.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/VisionizeHeaderLight.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/VisionizeHeaderLight.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/VisionizeHeaderLight.min.js"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "rollup": "^1.32.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luigiaxl/visionize-header-light.git"
  },
  "author": "Luigi Axl Wagas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luigiaxl/visionize-header-light/issues"
  },
  "homepage": "https://github.com/luigiaxl/visionize-header-light#readme"
}
